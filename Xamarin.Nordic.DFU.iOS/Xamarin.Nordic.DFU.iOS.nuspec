<?xml version="1.0"?>
<package>
  <metadata>
    <id>Xamarin.Nordic.DFU.iOS</id>
    <version>$version$</version>
    <title>Nordic OTA DFU for Xamarin.iOS</title>
    <authors>Laerdal Medical</authors>
    <owners>Laerdal Medical</owners>
    <icon>images/icon.png</icon>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      Enable over the air(OTA) device firmware update(DFU) for Nordic devices using iOS and Bluetooth Low Energy(BLE).
      This nuget is just a binding library for this native iOS Pod : https://github.com/NordicSemiconductor/IOS-Pods-DFU-Library
    </description>
    <releaseNotes></releaseNotes>
    <copyright>Copyright Laerdal Medical 2020</copyright>
    <repository type="git" url="https://github.com/framinosona/Xamarin.Nordic.DFU.iOS" branch="$branch$" commit="$commit$"/>
    <projectUrl>https://github.com/framinosona/Xamarin.Nordic.DFU.iOS</projectUrl>
    <license type="expression">MIT</license>
    <tags>Xamarin Nordic DFU iOS</tags>
    <dependencies>
      <group targetFramework="Xamarin.iOS"/>
    </dependencies>
  </metadata>
  <files>
    <file src="icon.png" target="images\"/>
    <file src="bin/*" target="lib\Xamarin.iOS" />
  </files>
</package>